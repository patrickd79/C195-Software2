<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="C195.SuccessfulLoginController">
   <children>
      <BorderPane layoutX="2.0" prefHeight="400.0" prefWidth="600.0">
         <left>
            <Button fx:id="addAppointmentBtn" mnemonicParsing="false" onAction="#goToAddAppointmentWindow" style="-fx-background-color: d6d6d6;" text="Add Appointment" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </BorderPane.margin></Button>
         </left>
         <bottom>
            <Button fx:id="addCustomerBtn" mnemonicParsing="false" onAction="#goToAddCustomerWindow" text="Add Customer" BorderPane.alignment="CENTER" />
         </bottom></BorderPane>
   </children>
</AnchorPane>
