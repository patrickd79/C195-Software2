<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="C195.AddCustomerController">
   <children>
      <Label layoutX="47.0" layoutY="21.0" text="Add Customer" />
      <ComboBox fx:id="addCustomerComboDivId" layoutX="379.0" layoutY="200.0" prefWidth="150.0" promptText="First Level Div" />
      <ComboBox fx:id="addCustomerComboCountry" layoutX="379.0" layoutY="91.0" prefWidth="150.0" promptText="Country" />
      <TextField fx:id="custNameField" layoutX="43.0" layoutY="79.0" promptText="Customer Name" />
      <TextField fx:id="custAddressField" layoutX="43.0" layoutY="129.0" promptText="Street Address" />
      <TextField fx:id="custPostalCodeField" layoutX="43.0" layoutY="175.0" promptText="Postal Code" />
      <TextField fx:id="custPhoneField" layoutX="43.0" layoutY="228.0" promptText="Phone  Number" />
      <TextField fx:id="customerCreatedByField" layoutX="39.0" layoutY="285.0" promptText="Created By" />
      <Button fx:id="addCustomerBtn" layoutX="379.0" layoutY="330.0" mnemonicParsing="false" onAction="#addCustomer" text="Add Customer" />
      <Label fx:id="addCustErrorField" layoutX="302.0" layoutY="11.0" prefHeight="39.0" prefWidth="275.0" textFill="#d80c0c" />
      <Button layoutX="83.0" layoutY="330.0" mnemonicParsing="false" onAction="#goToMainMenuWindow" text="Go To Main Menu" />
   </children>
